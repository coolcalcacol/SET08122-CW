{
    "name": "sudoku",
    "type": "module",
    "version": "1.0.0",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "license": "MIT",
    "author": {
        "name": "Alex Dumbreck",
        "email": "40552841@live.napier.ac.uk"
    },
    "packageManager": "yarn@3.5.0",
    "dependencies": {
        "chalk": "5.2.0",
        "readline": "1.3.0"
    },
    "scripts": {
        "build": "tsc",
        "compile": "esbuild dist/esm/index.js --bundle --outfile=dist/bundle.cjs --format=cjs --platform=node",
        "pkg": "pkg dist/bundle.cjs --output dist/sudoku",
        "lint": "eslint src/ --ext .ts",
        "lint:fix": "eslint src/ --ext .ts --fix",
        "prettier": "prettier --write src/**/*",
        "prettier:check": "prettier --check src/**/*",
        "start": "./dist/sudoku.exe",
        "dev": "yarn build && node ./dist/esm/index.js",
        "export": "yarn build && yarn compile && yarn pkg"
    },
    "devDependencies": {
        "@types/node": "18.14.6",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "esbuild": "0.17.14",
        "eslint": "8.35.0",
        "eslint-config-prettier": "8.6.0",
        "pkg": "5.8.0",
        "prettier": "2.8.4",
        "typescript": "4.9.5"
    }
}
